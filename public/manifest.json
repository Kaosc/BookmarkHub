{
  "manifest_version": 3,
  "name": "Bookmark Hub",
  "description": "A simple chrome extension to hold your bookmarks together.",
  "version": "1.1.0",
  "action": {
    "default_popup": "index.html",
    "default_title": "Bookmark Hub"
  },
  "icons": {
    "16": "/icons/icon_16.png",
    "32": "/icons/icon_32.png",
    "48": "/icons/icon_48.png",
    "128": "/icons/icon_128.png"
  },
  "permissions": [
    "activeTab"
  ]
}